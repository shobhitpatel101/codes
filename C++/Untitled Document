// C++ program to solve Gold Mine problem 
#include<bits/stdc++.h> 
using namespace std; 

void divcity(int gold[][3])
{
   
}

int main() 
{ 
	int gold[3][3]= {{1,10,3},
                    {2,5,3},
                    {3,1,1}}; 
	
	int i=0,j=0;
    int g[3][3];
    
    int count=0;
    
    g[0][0]=1;
    while(count<9)
    {
       int a1=gold[i][j]-gold[i][j+1];
       int a2=gold[i][j]-gold[i][j-1];
       int a3=gold[i][j]-gold[i+1][j];
       int a4=gold[i][j]-gold[i-1][j];
       
       if(j+1<3)a1=999;
       if(j-1<3)a2=999;
       if(i+1<3)a3=999;
       if(i-1<3)a4=999;
       
        if(a2<a1&&a2<a3&&a2<a4)
           { g[j-1][j]=1;
             j--;
           }
            
        else if(a1<a2&&a1<a3&&a1<a4)
            {g[i][j+1]=1;
             j++;
            }
            
        else if(a3<a1&&a3<a2&&a3<a4)
        {
            g[i+1][j]=1;
            i++;
        }
        else if(a4<a1&&a4<a2&&a4<a3)
        {
            g[i-1][j]=1;
            i--;
        }
            
            
            count++;
    }
    
    for(int i1=0;i1<3;i1++)
    {
        for(int j1=0;j1<3;j1++)
        cout<<g[i1][j1]<<  " ";
        cout<<endl;
    }
	
	return 0; 
} 

